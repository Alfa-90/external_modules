<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_picking_type_form_warehouse_app" model="ir.ui.view">
            <field name="name">Picking Types</field>
            <field name="model">stock.picking.type</field>
            <field name="inherit_id" ref="stock.view_picking_type_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group/field[@name='default_location_src_id']/.." position="after">

                    <group string="Info PDA">
                        <field name="show_in_pda"/>
                        <field name="short_name"/>
                        <!--field name="need_confirm"/>
                        <field name="process_from_tree"/-->
                    </group>
                </xpath>

            </field>
        </record>

        <record id="view_picking_inherit_form_warehouse_app" model="ir.ui.view">
            <field name="name">stock.picking.form.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='action_assign']" position="after">

                       <button name="force_availibility"
                               attrs="{'invisible': [('need_force', '!=', True)]}"
                               string="Force Availability" type="object" groups="stock.group_stock_manager"/>
                </xpath>


                <field name="location_id" position="attributes">
                    <attribute name="invisible">0</attribute>
                </field>
                <field name="location_dest_id" position="attributes">
                    <attribute name="invisible">0</attribute>
                </field>
                <field name="partner_id" position="after">
                    <field name="user_id"/>
                    <field name="need_force"/>

                </field>
                 <xpath expr="//tree/field[@name='reserved_availability']" position="after">
                    <field name="forced_move_line"/>
                      <field name="is_initial_demand_editable"/>
                </xpath>
                <xpath expr="//tree/field[@name='qty_done']" position="before">
                    <field name="forced_move_line"/>
                     <field name="is_initial_demand_editable"/>
                </xpath>
                <xpath expr="//field[@name='move_lines']/tree" position="attributes">
                    <attribute name="decoration-danger">(state != 'done' and quantity_done &gt; reserved_availability and show_reserved_availability) or forced_move_line</attribute>
                </xpath>



        </field>
        </record>



    </data>
</odoo>