<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_product_template_stock_apk" model="ir.ui.view">
            <field name="name">Product Template Form stock apk</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='virtual_stock_conservative']" position="after">
                    <field name="need_check"/>
                </xpath>
            </field>
        </record>


    </data>
</odoo>
