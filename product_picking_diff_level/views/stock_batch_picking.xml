<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_batch_picking_form_diff_level" model="ir.ui.view">
            <field name="name">view.batch.picking.form.diff_level</field>
            <field name="model">stock.batch.picking</field>
            <field name="inherit_id" ref="stock_batch_picking.stock_batch_picking_form"/>
            <field name="arch" type="xml">
                <field name="picker_id" position="after">
                    <field name="diff_level"/>
                </field>
            </field>
    </record>

    <record id="view_batch_picking_tree_diff_level" model="ir.ui.view">
        <field name="name">view.batch.picking.tree.diff_level</field>
        <field name="model">stock.batch.picking</field>
        <field name="inherit_id" ref="stock_batch_picking.stock_batch_picking_tree"/>
        <field name="arch" type="xml">
            <field name="state" position="before">
                <field name="diff_level"/>
            </field>
        </field>
    </record>
</odoo>
