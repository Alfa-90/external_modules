<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_move_picking_form_diff_level" model="ir.ui.view">
        <field name="name">stock.move.form.view.diff_level</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_picking_form"/>
        <field name="arch" type="xml">
            <field name="move_dest_ids" position="before">
                <field name="picking_code"/>
                <field name="diff_level" attrs = "{'invisible': [('picking_code', '!=', 'internal')]}"/>
            </field>
        </field>
    </record>
</odoo>
